<table id="productsTable">
    <tr>
      <th id="id">ID</th>
      <th id="timestampHeader">Timestamp</th>
      <th id="nombreHeader">Nombre</th>
      <th id="descripcionHeader">Descripción</th>
      <th id="codigoHeader">Código</th>
      <th id="fotoHeader">Foto</th>
      <th id="precioHeader">Precio</th>
      <th id="stockHeader">Stock</th>      
    </tr>
    <% productos.forEach(item=> { %>
      <tr>
        <td>
          <%= item.id %>
        </td>
        <td>
          <%= item.timestamp %>
        </td>
        <td>
          <%= item.nombre %>
        </td>
        <td>
          <%= item.descripcion %>
        </td>
        <td>
          <%= item.codigo %>
        </td>
        <td>
          <img src=<%= item.foto %>>
        </td>
        <td>$ 
          <%= item.precio %>
        </td>
        <td>
          <%= item.stock %>
        </td>        
      </tr>
      <% }) %>
  </table>